            <VBox fx:id="OrbitHolders" prefHeight="370.0" prefWidth="516.0" spacing="10.0" style="-fx-background-color: transparent; -fx-background-radius: 10px;" stylesheets="@style.css">
               <padding>
                  <Insets top="10.0" />
               </padding>
            </VBox>
         </content>
      </ScrollPane>

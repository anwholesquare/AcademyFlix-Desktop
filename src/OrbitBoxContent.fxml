      <Button fx:id="btnAddBox" alignment="TOP_CENTER" layoutX="250.0" layoutY="108.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="277.0" style="-fx-background-color: #00A8F0;" text="Add a new Orbit Box" textAlignment="CENTER" textFill="WHITE" wrapText="true">

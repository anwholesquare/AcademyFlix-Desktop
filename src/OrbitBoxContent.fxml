<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="OrbitBoxContentController">
    <children>
      <ImageView fitHeight="600.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@img/10.png" />
         </image>
      </ImageView>
      <Button fx:id="orbitBtn" layoutX="16.0" layoutY="157.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="190.0" style="-fx-background-color: transparent;" />
        <Button fx:id="contentBtn" layoutX="19.0" layoutY="239.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="190.0" style="-fx-background-color: transparent;" />
        <Button fx:id="generalBtn" layoutX="16.0" layoutY="314.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="190.0" style="-fx-background-color: transparent;" />
        <Button fx:id="orbitDashBtn" layoutX="16.0" layoutY="392.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="190.0" style="-fx-background-color: transparent;" />
  <Label fx:id="LabelTitle" layoutX="249.0" layoutY="45.0" prefHeight="29.0" prefWidth="311.0" text="Ethical Hacking" textFill="WHITE">
         <font>
            <Font size="24.0" />
